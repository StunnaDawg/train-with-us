{
  "expo": {
    "name": "train-with-us",
    "slug": "train-with-us",
    "version": "0.0.12",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#071C38"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      "expo-apple-authentication",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps._some_id_here_"
        }
      ],
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "841430162379-qukd112j7piosnltl3bqrptj9l0shckg.apps.googleusercontent.com"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stunnacat.trainwithus",
      "buildNumber": "1",
      "usesAppleSignIn": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["tel", "telprompt"]
      },
      "android": {
        "adaptiveIcon": {
          "backgroundColor": "#071C38",
          "foregroundImage": "./assets/images/adaptive-icon.png"
        }
      },
      "web": {
        "favicon": "./assets/images/favicon.png"
      },
      "extra": {
        "eas": {
          "projectId": "a551e684-d06e-4bb1-bd40-d83456f2c2fe"
        }
      }
    }
  }
}
